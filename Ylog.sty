%
% Some macros from plain
%
\newskip\plaincentering \plaincentering=0pt plus 1000pt minus 1000pt
\newcommand{\eqalign}[1]{\null\,\vcenter{\openup\jot\m@th
  \ialign{\strut\hfil$\displaystyle{##}$&$\displaystyle{{}##}$\hfil
      \crcr#1\crcr}}\,}
\newcommand{\eqalignno}[1]{\displ@y \tabskip\plaincentering
  \halign to\displaywidth{\hfil$\@lign\displaystyle{##}$\tabskip\z@skip
    &$\@lign\displaystyle{{}##}$\hfil\tabskip\plaincentering
    &\llap{$\@lign##$}\tabskip\z@skip\crcr
    #1\crcr}}
\newcommand{\leqalignno}[1]{\displ@y \tabskip\plaincentering
  \halign to\displaywidth{\hfil$\@lign\displaystyle{##}$\tabskip\z@skip
    &$\@lign\displaystyle{{}##}$\hfil\tabskip\plaincentering
    &\kern-\displaywidth\rlap{$\@lign##$}\tabskip\displaywidth\crcr
    #1\crcr}}
%
% \axlab and \ecr, in connection with \displaylines, is used to construct
% lists of axioms
